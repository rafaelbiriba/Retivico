package {	class Client {		public var coeficiente:Number = 0;		function Client(coeficienteVar:Number){ coeficiente = coeficienteVar; }	    public var banda:Number = 0; 	   	   	public function onBWCheck(... rest):Number { 			return 0; 	    } 			    public function onBWDone(... rest):void { 			var p_bw:Number = 0; 			trace("BIRIBAAA>>> rest.length : " + rest.length);			if (rest.length > 0){				if(rest[0] > 0) p_bw = (p_bw + rest[0])/2;				if(rest[1] > 1) p_bw = (p_bw + rest[1])/2;				if(rest[2] > 2) p_bw = (p_bw + rest[2])/2;				if(rest[3] > 3) p_bw = (p_bw + rest[3])/2;			} 			p_bw = p_bw - (p_bw * coeficiente)/100;		    if((banda > 0) && (p_bw < banda*coeficiente) && (banda < p_bw*coeficiente)){		    	banda = Math.round((banda + p_bw)/2);		    	trace("(IF) bandwidth = " + banda + " Kbps.");		    }		    else{		    	banda = Math.round(p_bw);		    	trace("(ELSE) bandwidth = " + banda + " Kbps.");		    }	    } 			    public function getBanda():Number {	    	return banda;	    }	}}